.PHONY: distrib fulldistrib top links rebuild

distrib:
	cd ..; $(MAKE) distrib

fulldistrib:
	cd ..; $(MAKE) fulldistrib

top:
	cd ..; $(MAKE) top

tags:
	cd ..; $(MAKE) tags

links: 
	cd ..; $(MAKE) links

rebuild: 
	cd ..; $(MAKE) rebuild
