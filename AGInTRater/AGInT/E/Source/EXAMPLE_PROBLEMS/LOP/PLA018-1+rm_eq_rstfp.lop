    holds(on(X, Y), State),
    holds(clear(X), State),
    holds(empty, State).

# pickup_3, axiom.
holds(on(X, Y), do(pickup(Z), State)) <- 
    holds(on(X, Y), State),
    differ(X, Z).

# pickup_4, axiom.
