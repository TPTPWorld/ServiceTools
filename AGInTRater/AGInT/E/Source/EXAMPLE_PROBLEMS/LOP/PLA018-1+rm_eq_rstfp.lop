# and_definition, axiom.
holds(and(X, Y), State) <- 
    holds(X, State),
    holds(Y, State).

# pickup_1, axiom.
holds(holding(X), do(pickup(X), State)) <- 
    holds(empty, State),
    holds(clear(X), State),
    differ(X, table).

# pickup_2, axiom.
